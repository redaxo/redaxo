{
    "require": {
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8",
        "filp/whoops": "^2.7",
        "psr/log": "^1.0",
        "ramsey/http-range": "dev-legacy",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^4.4",
        "symfony/polyfill-mbstring": "^1.14",
        "symfony/var-dumper": "^4.4",
        "symfony/yaml": "^4.4",
        "voku/anti-xss": "^4.1"
    },

    "replace": {
        "symfony/polyfill-iconv": "*"
    },

    "config": {
        "autoloader-suffix": "RedaxoCore",
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/redaxo/http-range"
        }
    ]
}
