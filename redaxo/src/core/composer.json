{
    "type": "project",

    "require": {
        "composer/ca-bundle": "^1.5.7",
        "enshrined/svg-sanitize": "^0.21",
        "erusev/parsedown": "^1.7.4",
        "erusev/parsedown-extra": "^0.8.1",
        "filp/whoops": "^2.18.3",
        "lbuchs/webauthn": "^2.2.0",
        "psr/http-message": "^1.1",
        "psr/log": "^3.0.2",
        "ramsey/http-range": "^1.2.1",
        "symfony/console": "^6.4.23",
        "symfony/http-foundation": "^6.4.23",
        "symfony/var-dumper": "^6.4.23",
        "symfony/yaml": "^6.4.23",
        "voku/anti-xss": "^4.1.42",
        "voku/portable-utf8": "dev-master as 6.0.14-dev"
    },

    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },

    "config": {
        "autoloader-suffix": "RedaxoCore",
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.1.13"
        },
        "platform-check": false,
        "prepend-autoloader": false
    }
}
